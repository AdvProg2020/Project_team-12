<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JavaProject.Controller.SellerAuctionsManagementController">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ListView fx:id="auctionsList" prefHeight="560.0" HBox.hgrow="ALWAYS" />
            <VBox prefHeight="560.0" spacing="10.0" HBox.hgrow="ALWAYS">
               <children>
                  <ListView fx:id="productsList" prefHeight="521.0" prefWidth="141.0" VBox.vgrow="ALWAYS" />
                  <Button fx:id="addProductButton" mnemonicParsing="false" onAction="#addProduct" prefHeight="40.0" prefWidth="210.0" text="Add product" />
               </children>
            </VBox>
            <VBox layoutX="10.0" layoutY="10.0" prefHeight="560.0" spacing="10.0" HBox.hgrow="ALWAYS">
               <children>
                  <TextField fx:id="startDateField" prefHeight="40.0" prefWidth="200.0" promptText="start date" />
                  <TextField fx:id="endDateField" prefHeight="40.0" prefWidth="200.0" promptText="end date" />
                  <TextField fx:id="percentField" prefHeight="40.0" prefWidth="200.0" promptText="percent" />
                  <Button fx:id="addButton" mnemonicParsing="false" onAction="#requestAddOrEditAuction" prefHeight="39.0" prefWidth="220.0" text="Request add" VBox.vgrow="NEVER">
                     <VBox.margin>
                        <Insets right="1.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="editButton" mnemonicParsing="false" onAction="#requestAddOrEditAuction" prefHeight="39.0" prefWidth="220.0" text="Request edit" VBox.vgrow="NEVER" />
                  <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#requestDeleteAuction" prefHeight="39.0" prefWidth="220.0" text="Request delete" />
                  <Label fx:id="statusLabel" alignment="CENTER" prefHeight="40.0" prefWidth="200.0" text="Status" />
                  <ListView fx:id="auctionProductsList" prefHeight="280.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
                  <Button fx:id="removeProductButton" mnemonicParsing="false" onAction="#removeProduct" prefHeight="40.0" prefWidth="220.0" text="Remove product" />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
