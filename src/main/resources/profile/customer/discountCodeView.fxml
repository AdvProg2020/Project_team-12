<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JavaProject.Controller.DiscountCodeViewController">
   <children>
      <HBox alignment="CENTER" prefHeight="600.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ListView fx:id="discountCodesList" prefHeight="600.0" prefWidth="336.0" HBox.hgrow="ALWAYS">
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin>
            </ListView>
            <VBox alignment="CENTER" prefHeight="600.0" prefWidth="266.0" spacing="20.0" HBox.hgrow="ALWAYS">
               <children>
                  <TextField fx:id="codeField" promptText="code" VBox.vgrow="NEVER" />
                  <TextField fx:id="startField" promptText="start date" VBox.vgrow="NEVER" />
                  <TextField fx:id="endField" promptText="end date" VBox.vgrow="NEVER" />
                  <TextField fx:id="percentField" promptText="percent" VBox.vgrow="NEVER" />
                  <TextField fx:id="maxDiscountField" promptText="maximum discount" VBox.vgrow="NEVER" />
                  <TextField fx:id="remainingField" promptText="remainig usages" VBox.vgrow="NEVER" />
               </children>
               <padding>
