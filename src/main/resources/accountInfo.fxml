<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" stylesheets="@accountInfo.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JavaProject.Controller.AccountInfoController">
   <children>
      <HBox prefHeight="600.0" prefWidth="600.0">
         <children>
            <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="600.0" prefWidth="279.0" spacing="8.0">
               <children>
                  <ImageView fx:id="imageView" fitHeight="144.0" fitWidth="151.0" onMouseClicked="#chooseImage" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/person_100px.png" />
                     </image>
                  </ImageView>
                  <Button mnemonicParsing="false" onAction="#resetImageToDefault" text="Reset Profile Image">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
                  <Label fx:id="statusLabel" prefHeight="39.0" prefWidth="224.0" />
               </children>
            </VBox>
            <Region prefHeight="600.0" prefWidth="101.0" />
            <VBox alignment="CENTER" prefHeight="600.0" prefWidth="284.0" spacing="10.0">
               <children>
                  <Region prefHeight="31.0" prefWidth="238.0" />
                  <TextField fx:id="usernameField" prefHeight="8.0" prefWidth="100.0" promptText="username">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </TextField>
                  <TextField fx:id="firstNameField" promptText="first name">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </TextField>
                  <TextField fx:id="lastNameField" promptText="last name">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </TextField>
                  <TextField fx:id="emailAddressField" promptText="email address">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </TextField>
                  <TextField fx:id="phoneNumberField" promptText="phone number">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </TextField>
                  <TextField fx:id="companyNameField" promptText="company name">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </TextField>
                  <PasswordField fx:id="currentPasswordField" promptText="current password">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </PasswordField>
                  <PasswordField fx:id="newPasswordField" promptText="new password">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </PasswordField>
                  <TextField fx:id="accountTypeField" promptText="account type">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </TextField>
                  <TextField fx:id="balanceField" promptText="balance">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </TextField>
                  <Button mnemonicParsing="false" onAction="#update" prefHeight="29.0" prefWidth="88.0" text="update">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <Region prefHeight="600.0" prefWidth="95.0" />
         </children>
      </HBox>
   </children>
</AnchorPane>
