<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane prefHeight="600.0" prefWidth="800.0" stylesheets="@profile.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JavaProject.Controller.SellerSaleHistoryController">
   <children>
       <ImageView fitHeight="600.0" fitWidth="800.0" opacity="0.6" pickOnBounds="true" preserveRatio="true">
           <image>
               <Image url="@../Images/profileBG.jpg" />
           </image>
       </ImageView>
      <TableView fx:id="logsTable" layoutX="25.0" layoutY="25.0" prefHeight="275.0" prefWidth="750.0">
        <columns>
          <TableColumn fx:id="logIDColumn" prefWidth="200.0" resizable="false" text="LogID" />
          <TableColumn fx:id="dateColumn" prefWidth="150.0" resizable="false" text="Date" />
            <TableColumn fx:id="buyerColumn" prefWidth="150.0" resizable="false" text="Buyer" />
            <TableColumn fx:id="moneyColumn" prefWidth="125.0" resizable="false" text="Money" />
            <TableColumn fx:id="discountColumn" prefWidth="120.0" resizable="false" text="Discount" />
        </columns>
      </TableView>
      <TableView fx:id="productsTable" layoutX="25.0" layoutY="325.0" prefHeight="250.0" prefWidth="750.0">
        <columns>
          <TableColumn fx:id="productColumn" prefWidth="175.0" resizable="false" text="Product" />
          <TableColumn fx:id="quantityColumn" prefWidth="125.0" resizable="false" text="Quantity" />
            <TableColumn fx:id="productMoneyColumn" prefWidth="125.0" resizable="false" text="Money" />
            <TableColumn fx:id="productDiscountColumn" prefWidth="125.0" resizable="false" text="Discount" />
            <TableColumn fx:id="auctionIDColumn" prefWidth="200.0" resizable="false" text="AuctionID" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
